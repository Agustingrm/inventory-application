<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <%- include('partials/sidebar'); %>
    <div class='content'>
      <h1><%= title %></h1>
      <p>Welcome to <%= brand.name %></p>
      <% let i = 0 %> <% brand_items.forEach(()=>{ %>
        <ul>
          <li>
            <a href="<%=brand_items[i].url%>"><strong> <%= brand_items[i].name %> </strong></a>
            <p><%= brand_items[i].description %></p>
          </li>
        </ul>
        <% i += 1 %> <%}) %>
    </div>
  </body>
</html>